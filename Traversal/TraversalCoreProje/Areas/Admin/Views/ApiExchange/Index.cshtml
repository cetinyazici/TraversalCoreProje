@model IEnumerable<BookingExchange2ViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h1 class="text-center">ApiExchange Kur Bilgileri</h1>
<table class="table table-bordered table-hover">
    <tr>
        <th>KUR ADI</th>
        <th>KUR ORANI</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }
</table>